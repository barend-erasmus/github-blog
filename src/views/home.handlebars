<div class="main-content">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                {{#each posts as |value key|}}
                <article class="post">
                    <div class="post-thumb">
                        <a href="/post/{{value.key}}"><img src="http://via.placeholder.com/745x470" alt=""></a>
                        <a href="/post/{{value.key}}" class="post-thumb-overlay text-center">
                            <div class="text-uppercase text-center">View Post</div>
                        </a>
                    </div>
                    <div class="post-content">
                        <header class="entry-header text-center text-uppercase">
                            <h6><a href="#"> Travel</a></h6>
                            <h1 class="entry-title"><a href="/post/{{value.key}}">{{value.title}}</a></h1>
                        </header>
                        <div class="entry-content">
                            <p>{{value.description}}</p>

                            <div class="btn-continue-reading text-center text-uppercase">
                                <a href="/post/{{value.key}}" class="more-link">Continue Reading</a>
                            </div>
                        </div>
                        <div class="social-share">
                            <span class="social-share-title pull-left text-capitalize">By {{value.author}} On {{value.displayTimestamp}}</span>
                            <ul class="text-center pull-right">
                                <li><a class="s-facebook" href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a class="s-twitter" href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a class="s-google-plus" href="#"><i class="fa fa-google-plus"></i></a></li>
                                <li><a class="s-linkedin" href="#"><i class="fa fa-linkedin"></i></a></li>
                                <li><a class="s-instagram" href="#"><i class="fa fa-instagram"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </article>
                {{/each}}
            </div>
